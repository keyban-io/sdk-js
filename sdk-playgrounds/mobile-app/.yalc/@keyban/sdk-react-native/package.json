{
  "name": "@keyban/sdk-react-native",
  "version": "0.0.1",
  "description": "React Native SDK for Keyban MPC wallet solution.",
  "repository": {
    "type": "git",
    "url": "https://github.com/keyban-io/dap.git"
  },
  "homepage": "https://github.com/keyban-io/dap",
  "bugs": "https://github.com/keyban-io/dap/issues",
  "author": {
    "name": "Keyban",
    "email": "keyban@google.com",
    "url": "https://keyban.com"
  },
  "keywords": [
    "hedera",
    "plygon",
    "eddsa",
    "ecdsa",
    "mpc",
    "crypto"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./lib/web": {
      "types": "./lib/web/index.d.ts",
      "import": "./lib/web/index.mjs",
      "default": "./lib/web/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "compile:proto": "node ./src/eddsa/wasmBridge/script/compileProto.js",
    "build": "NODE_ENV=production tsup",
    "build:docs": "typedoc --out docs ./src/index.ts",
    "dev": "tsup --watch",
    "format": "biome check --apply ./src",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "changeset": "changeset"
  },
  "dependencies": {
    "@keyban/sdk-base": "file:.yalc/@keyban/sdk-base",
    "long": "^5.2.3",
    "react-native-buffer": "^6.0.3"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-react-bridge": "*",
    "react-native-webview": "*"
  },
  "yalcSig": "40d2ecaa54457b0073ba6ad718cd6026"
}
