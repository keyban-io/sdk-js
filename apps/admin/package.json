{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "refine dev",
    "build": "tsc && refine build",
    "start": "refine start",
    "refine": "refine",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "pnpm lint --fix",
    "intl:extract": "formatjs extract 'src/**/*.ts*' --ignore='**/*.d.ts' --out-file src/lang/en-US.json --id-interpolation-pattern '[sha512:contenthash:base64:6]' --pragma intl-meta",
    "intl:compile:en-US": "formatjs compile src/lang/en-US.json --ast --out-file src/lang/en-US.ast.json",
    "intl:compile:fr-FR": "formatjs compile src/lang/fr-FR.json --ast --out-file src/lang/fr-FR.ast.json"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
