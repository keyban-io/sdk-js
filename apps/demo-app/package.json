{
  "name": "demo-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@keyban/sdk-react": "workspace:*",
    "@types/node": "^16.18.101",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.25.1",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "reinstall-eddsa-wasm": "earthly --no-cache ../../+get-eddsa-wasm",
    "reinstall-ecdsa-wasm": "earthly --no-cache ../../+get-ecdsa-wasm && pnpm install",
    "reinstall-packages": "pnpm --prefix ../../packages/sdk-base build && pnpm --prefix ../../packages/sdk-react build",
    "meta-build": "earthly ../../+build && earthly ../../+get-eddsa-wasm && earthly ../../+get-ecdsa-wasm && pnpm --prefix ../../packages/sdk-base build && pnpm --prefix ../../packages/sdk-react build && pnpm install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2"
  }
}
