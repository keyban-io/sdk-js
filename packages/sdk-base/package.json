{
  "name": "@keyban/sdk-base",
  "version": "0.0.0",
  "description": "Keyban SDK Base provides core functionalities for the MPC wallet solution, supporting web and Node.js apps with TypeScript, custom storage, and Ethereum blockchain integration.",
  "homepage": "https://docs.demo.keyban.io/",
  "author": {
    "name": "Keyban",
    "email": "it@keyban.io",
    "url": "https://keyban.io"
  },
  "keywords": [
    "polygon",
    "ecdsa",
    "mpc",
    "tss",
    "crypto",
    "typescript",
    "ethereum",
    "wallet",
    "blockchain"
  ],
  "license": "Apache-2.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./graphql": {
      "types": "./dist/graphql.d.ts",
      "import": "./dist/graphql.mjs",
      "require": "./dist/graphql.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "codegen": "graphql-codegen",
    "dev": "tsup --watch --onSuccess 'tsc --emitDeclarationOnly --declaration'",
    "build": "tsup",
    "prepack": "pnpm install @keyban/ecdsa-wasm-client@latest && pnpm install @keyban/ecdsa-wasm-client@latest",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "pnpm lint --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.11.10",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@keyban/ecdsa-wasm-client": "workspace:*",
    "graphql-ws": "^5.16.0",
    "subscriptions-transport-ws": "^0.11.0",
    "viem": "^2.21.51"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typed-document-node": "^5.0.12",
    "@types/node": "^22.10.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "globals": "^15.12.0",
    "prettier": "^3.4.1",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0"
  }
}
