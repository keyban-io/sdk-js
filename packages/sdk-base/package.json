{
  "name": "@keyban/sdk-base",
  "version": "0.0.1",
  "description": "Base SDK from Keyban MPC wallet solution.",
  "repository": {
    "type": "git",
    "url": "https://github.com/keyban-io/dap.git"
  },
  "homepage": "https://github.com/keyban-io/dap",
  "bugs": "https://github.com/keyban-io/dap/issues",
  "author": {
    "name": "Keyban",
    "email": "keyban@google.com",
    "url": "https://keyban.com"
  },
  "keywords": [
    "hedera",
    "plygon",
    "eddsa",
    "ecdsa",
    "mpc",
    "crypto"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsup && yalc publish",
    "build:docs": "typedoc --out docs",
    "dev": "tsup --watch --onSuccess 'tsc --emitDeclarationOnly --declaration && yalc push'",
    "format": "biome check --apply ./src",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "changeset": "changeset"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.12.7",
    "@vitest/coverage-v8": "^1.4.0",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
