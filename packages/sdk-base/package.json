{
  "name": "@keyban/sdk-base",
  "version": "0.0.0",
  "description": "Base SDK from Keyban MPC wallet solution.",
  "repository": {
    "type": "git",
    "url": "https://github.com/keyban-io/dap.git"
  },
  "homepage": "https://github.com/keyban-io/dap",
  "bugs": "https://github.com/keyban-io/dap/issues",
  "author": {
    "name": "Keyban",
    "email": "it@keyban.io",
    "url": "https://keyban.io"
  },
  "keywords": [
    "hedera",
    "plygon",
    "ecdsa",
    "mpc",
    "crypto"
  ],
  "license": "Apache-2.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "codegen": "graphql-codegen",
    "dev": "tsup --watch --onSuccess 'tsc --emitDeclarationOnly --declaration'",
    "build": "tsup",
    "format": "prettier --write ./src",
    "prepack": "pnpm install @keyban/ecdsa-wasm-client@$(pnpm view @keyban/ecdsa-wasm-client version)"
  },
  "dependencies": {
    "@keyban/ecdsa-wasm-client": "workspace:*",
    "viem": "^2.20.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/near-operation-file-preset": "^3.0.0",
    "@graphql-codegen/typescript-generic-sdk": "^4.0.1",
    "@types/node": "^20.16.0",
    "prettier": "^3.3.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
