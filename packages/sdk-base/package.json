{
  "name": "@keyban/sdk-base",
  "version": "0.0.1",
  "description": "Base SDK from Keyban MPC wallet solution.",
  "repository": {
    "type": "git",
    "url": "https://github.com/keyban-io/dap.git"
  },
  "homepage": "https://github.com/keyban-io/dap",
  "bugs": "https://github.com/keyban-io/dap/issues",
  "author": {
    "name": "Keyban",
    "email": "keyban@google.com",
    "url": "https://keyban.com"
  },
  "keywords": [
    "hedera",
    "plygon",
    "eddsa",
    "ecdsa",
    "mpc",
    "crypto"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsup",
    "dev": "tsup --watch --onSuccess 'tsc --emitDeclarationOnly --declaration'",
    "format": "prettier --write ./src",
    "changeset": "changeset"
  },
  "dependencies": {
    "ecdsa-wasm-client": "workspace:*",
    "eddsa-wasm-client": "workspace:*",
    "viem": "^2.17.11"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@types/node": "22.4.1",
    "prettier": "^3.3.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
