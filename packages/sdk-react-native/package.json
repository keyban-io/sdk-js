{
  "name": "@keyban/sdk-react-native",
  "version": "0.0.1",
  "description": "React Native SDK for Keyban MPC wallet solution.",
  "repository": {
    "type": "git",
    "url": "https://github.com/keyban-io/dap.git"
  },
  "homepage": "https://github.com/keyban-io/dap",
  "bugs": "https://github.com/keyban-io/dap/issues",
  "author": {
    "name": "Keyban",
    "email": "keyban@google.com",
    "url": "https://keyban.com"
  },
  "keywords": [
    "hedera",
    "plygon",
    "eddsa",
    "ecdsa",
    "mpc",
    "crypto"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./dist/web": {
      "types": "./dist/web/index.d.ts",
      "import": "./dist/web/index.mjs",
      "default": "./dist/web/index.js"
    },
    "./dist/wasmBridge": {
      "types": "./dist/wasmBridge/index.d.ts",
      "import": "./dist/wasmBridge/index.mjs",
      "default": "./dist/wasmBridge/index.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {}
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "compile:proto": "./compile.sh",
    "build": "NODE_ENV=production tsup && yalc publish",
    "build:docs": "typedoc --out docs",
    "dev": "tsup --no-dts --watch --onSuccess 'tsc --emitDeclarationOnly --declaration && yalc push'",
    "format": "biome check --apply ./src",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest watch",
    "changeset": "changeset"
  },
  "dependencies": {
    "@keyban/sdk-base": "workspace:*",
    "eddsa-wasm-client": "workspace:*",
    "long": "^5.2.3",
    "react-native-buffer": "^6.0.3"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@types/node": "^20.12.7",
    "@types/react": "^18.0.21",
    "@vitest/browser": "^1.6.0",
    "@vitest/coverage-v8": "^1.4.0",
    "protobufjs": "^7.2.6",
    "protobufjs-cli": "^1.1.2",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5",
    "vite-plugin-node-polyfills": "^0.22.0",
    "vitest": "^1.4.0",
    "webdriverio": "^8.38.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "*",
    "react": "*",
    "react-dom": "*",
    "react-native": "*",
    "react-native-react-bridge": "*",
    "react-native-webview": "*"
  }
}
