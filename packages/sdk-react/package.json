{
  "name": "@keyban/sdk-react",
  "version": "0.0.0",
  "description": "React SDK from Keyban MPC wallet solution.",
  "repository": {
    "type": "git",
    "url": "https://github.com/keyban-io/dap.git"
  },
  "homepage": "https://github.com/keyban-io/dap",
  "bugs": "https://github.com/keyban-io/dap/issues",
  "author": {
    "name": "Keyban",
    "email": "it@keyban.io",
    "url": "https://keyban.io"
  },
  "keywords": [
    "hedera",
    "plygon",
    "ecdsa",
    "mpc",
    "crypto"
  ],
  "license": "Apache-2.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "dev": "tsup --watch --onSuccess 'tsc --emitDeclarationOnly --declaration'",
    "build": "tsup",
    "format": "prettier --write ./src",
    "prepack": "pnpm install @keyban/sdk-base@$(pnpm view @keyban/sdk-base version)"
  },
  "dependencies": {
    "@keyban/sdk-base": "workspace:*"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.1",
    "@types/node": "^20.16.0",
    "@types/react": "^18.0.21",
    "prettier": "^3.3.3",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
