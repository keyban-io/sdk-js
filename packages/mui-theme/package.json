{
  "name": "@keyban/mui-theme",
  "version": "0.0.0",
  "description": "A custom MUI theme for Keyban projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/keyban-io/dap.git"
  },
  "homepage": "https://github.com/keyban-io/dap",
  "bugs": "https://github.com/keyban-io/dap/issues",
  "author": {
    "name": "Keyban",
    "email": "it@keyban.io",
    "url": "https://keyban.io"
  },
  "keywords": [
    "mui",
    "theme",
    "custom",
    "keyban"
  ],
  "license": "Apache-2.0",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist/**/*",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsup"
  },
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/chivo": "^5.1.0",
    "@fontsource/nunito": "^5.1.0",
    "@mui/material": "^6.1.3"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.6.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
